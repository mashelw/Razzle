<div class="container">
        <!-- Modal Number Login -->
        <div class="modal fade" id="myModal" tabindex="-1" data-focus-on="input:first" style="display: none;">>
            <div class="modal-dialog modal-sm">        
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                      <h4 class="modal-title">Enter Your Phone Number</h4>
                    </div>
                    <div class="modal-body">
                        <form class="form-signin text-center" action="#myModalVerify" >       
                            <label for="phonenum"><h4>EXAMPLE: 14150001234</h4></label>
                            <input class="form-input" id="phone" type="tel" pattern="^\d{11}$" required >
                            <div class="modal-footer">
                                <button class="btn btn-md btn-primary btn-block" data-toggle="modal"  href="#myModalVerify" >SUBMIT</button>
                            
                            
                            </div>    
                        </form>
                    </div>           
                </div>     
            </div>
        </div>  
        <!-- Modal Verification -->
        <div class="modal fade" id="myModalVerify" tabindex="-1" data-focus-on="input:first" style="display: none;">>
            <div class="modal-dialog modal-sm">
        
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                      <h4 class="modal-title">Enter Your Six Digit Password</h4>
                    </div>
                    <div class="modal-body">
                        <form class="form-signin text-center" action="/home">       
                            <label for="phonenum"><h4>EXAMPLE: 888666</h4></label>
                            <input class="form-input" id="twoFactor" type="password" pattern="^\d{6}$" required >
                            <div class="modal-footer">
                               <button class="btn btn-md btn-primary btn-block" type="submit" >SUBMIT</button>
                            </div>    
                        </form>
                    </div>           
                </div>     
            </div>
        </div> 

    </div>

    <!-- Page Content -->
    <div class="container">
        <!-- Logo -->
        <div class="box text-center">
        <img src="img/RazzleLogo.png"> 
        </div>
        <div class="box">
            <!-- Carousel -->
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                  <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                  <li data-target="#myCarousel" data-slide-to="1"></li>
                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner">
                    <div class="item active">                
                        <img src="img/Infograph.png" alt="First slide">                
                    </div>
                    {{#each items}}
                    <div class="item">
                        <div class="jumbotron">
                            <div class="row text-center"> 
                            <div class="col-md-4 col-sm-6 hero"></div>               
                               
                                <div class="col-md-4 col-sm-6 hero">
                                    <div class="thumbnail">
                                        <img src={{this.link}}  alt={{this.item}}>
                                        <div class="caption">
                                            <h3>{{this.item}}</h3>
                                            <p>Tickets Purchased: {{this.purchasedTickets}}</p>
                                            <a data-toggle="modal" data-target="#myModal" class="btn btn-primary text-center">Login!</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-sm-6 hero"></div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
                    <!-- Left and right controls for Carousel-->
                    <a class="carousel-control" href="#myCarousel" data-slide="prev">
                      <span class="glyphicon glyphicon-chevron-left"></span>
                      <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" data-slide="next">
                      <span class="glyphicon glyphicon-chevron-right"></span>
                      <span class="sr-only">Next</span>
                    </a>
            </div>
        </div>
    </div>      
 